<section class="press">
  <div class="container">
    <div class="row">
      <div class="col-12">
        <div class="press__email">
          <div class="press__email-content">
            <h2>For press opportunities please email</h2>
            <a href="mailto:media@soteranalytics.com"><img alt="" src="data:image/svg+xml;base64," />media@soteranalytics.com</a>
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-12">
        {% for pressItem in list %}
          <a href="#" class="press__item">
            <div class="press__item-image"><img src="./assets/img/press/{{ pressItem.image }}" alt=""></div>
            <div class="press__item-content">
              <time>{{ pressItem.time }}</time>
              <h2>{{ pressItem.title }}</h2>
            </div>
          </a>
        {% endfor %}

      </div>
    </div>
    <div class="row">
      <div class="col-12">
        <div class="press__pagination">
          <div class="wp-pagenavi" role="navigation">
            <span aria-current="page" class="current">1</span>
            <a class="page larger" title="Page 2" href="https://soteranalytics.com/posts/page/2/">2</a>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>